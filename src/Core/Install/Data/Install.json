{
    "user": {
        "validate": {
            "email" : [
                {
                    "is.email" : true
                }
            ],
            "password" : [
                {
                    "string.length": ">=8"
                },
                {
                    "string.has.number": ">=1"
                },
                {
                    "string.has.uppercase": ">=1"
                },
                {
                    "string.has.lowercase": ">=1"
                },
                {
                    "string.has.symbol" : ">=1"
                }
            ]
        }
    }
}